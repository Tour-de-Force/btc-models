{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "login schema",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "email": {
      "type": "string",
      "format": "email"
    },
    "password": {
      "type": "string",
      "minLength": 8
    }
  },
  "required": [ "email", "password" ]
}
